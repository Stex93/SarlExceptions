package it.unito.di.sarlexceptions.examples.atm

import io.sarl.core.Initialize
import io.sarl.core.Lifecycle
import java.util.UUID

/** 
 * @author Giova
 * 
 */
agent BootAgent {
	uses Lifecycle

	on Initialize {
		var spaceId = UUID::randomUUID
		
		var gui = new ATMGUI
		gui.setup

		spawn(UserAgent, spaceId)
		spawn(Parser, spaceId)

		killMe
	}
}
