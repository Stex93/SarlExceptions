package it.unito.di.sarlexceptions.test.agents

import io.sarl.core.Initialize
import io.sarl.core.Lifecycle
import java.util.UUID

agent Boot {

	uses Lifecycle

	on Initialize {
		
		var spaceId = UUID::randomUUID
		
		
		spawn(Alice, spaceId)
		spawn(Bob, spaceId)
		spawn(Carol, spaceId)
		
		killMe
	
	}

}
