package it.unito.di.agents

import io.sarl.core.Behaviors
import io.sarl.core.DefaultContextInteractions
import io.sarl.core.Initialize
import it.unito.di.spaces.MySpace
import it.unito.di.spaces.MySpaceSpecification
import java.util.UUID

agent Bob {

	uses DefaultContextInteractions,Behaviors

	var comspace : MySpace

	on Initialize {

		comspace = defaultContext.getOrCreateSpaceWithSpec(typeof(MySpaceSpecification),
			occurrence.parameters.get(0) as UUID)

		comspace.printMessage("Hello from BOB")

	}

}
